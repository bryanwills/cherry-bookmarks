{
  "name": "cherry",
  "version": "0.0.8",
  "private": true,
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.22.0",
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-svelte"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "vite dev",
    "test": "vitest",
    "do": "NODE_OPTIONS='--import tsx' node",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "sync": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "bundle:cli": "esbuild src/cli/index.ts --bundle --minify --platform=node --target=node24 --outfile=cherry",
    "cherry": "tsx src/cli/index.ts",
    "lint": "prettier --check src scripts && eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "prettier:fix": "prettier --write src scripts",
    "fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@fontsource/inter": "5.2.8",
    "@lucide/svelte": "0.553.0",
    "@sveltejs/adapter-node": "5.4.0",
    "@sveltejs/kit": "2.48.5",
    "@sveltejs/package": "2.5.4",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@types/better-sqlite3": "7.6.13",
    "@types/debug": "4.1.12",
    "@types/estree": "1.0.8",
    "@types/tar": "6.1.13",
    "autoprefixer": "10.4.22",
    "axios": "1.13.2",
    "date-fns": "4.1.0",
    "domhandler": "5.0.3",
    "esbuild": "0.27.0",
    "esbuild-register": "3.6.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-svelte": "^3.13.0",
    "file-selector": "2.1.2",
    "globals": "16.5.0",
    "ky": "1.14.0",
    "nanoid": "5.1.6",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "sade": "1.8.1",
    "sass": "1.94.0",
    "svelte": "5.43.6",
    "svelte-check": "4.3.4",
    "svelte-file-dropzone": "2.0.9",
    "tiny-invariant": "1.3.3",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "uint8array-extras": "1.5.0",
    "undici": "7.16.0",
    "vite": "7.2.2",
    "vitest": "4.0.9"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "3.932.0",
    "@aws-sdk/lib-storage": "3.932.0",
    "@node-rs/argon2": "2.0.2",
    "better-sqlite3": "12.4.1",
    "csv-stringify": "6.6.0",
    "debug": "4.4.3",
    "dotenv": "17.2.3",
    "file-type": "21.1.0",
    "htmlparser2": "10.0.0",
    "tar": "7.5.2",
    "zod": "4.1.12"
  }
}
