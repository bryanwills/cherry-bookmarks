{
  "name": "cherry",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "scripts": {
    "start": "DEBUG=cherry:* node -r 'global-agent/bootstrap' node_modules/@sveltejs/kit/svelte-kit.js dev",
    "dev": "svelte-kit dev",
    "postinstall": "node scripts/font.mjs",
    "build": "svelte-kit build",
    "postbuild": "tree .vercel_build_output",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src scripts && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src scripts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/adapter-node": "1.0.0-next.73",
    "@sveltejs/kit": "1.0.0-next.324",
    "@types/better-sqlite3": "7.5.0",
    "@types/debug": "4.1.7",
    "@types/dotenv": "8.2.0",
    "@types/jsonwebtoken": "8.5.8",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "dotenv": "16.0.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "3.4.1",
    "global-agent": "3.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "sass": "1.51.0",
    "svelte": "3.48.0",
    "svelte-check": "2.7.0",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-vercel": "1.0.0-next.50",
    "better-sqlite3": "7.5.1",
    "debug": "4.3.4",
    "html-metadata-parser": "2.0.4",
    "jsonwebtoken": "8.5.1",
    "node-fetch": "3.2.4"
  }
}
