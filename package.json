{
  "name": "cherry",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": [
      "."
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "vite dev",
    "test": "NODE_OPTIONS='--loader tsx' uvu src .test.ts",
    "do": "NODE_OPTIONS='--loader tsx' node",
    "test:1": "uvu -r esbuild-register -r tsconfig-paths/register",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "sync": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "bundle:cli": "esbuild src/cli/index.ts --bundle --minify --platform=node --target=node16 --outfile=cherry",
    "lint": "prettier --check src scripts && eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "prettier:fix": "prettier --plugin-search-dir=. --write src scripts",
    "fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "devDependencies": {
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/firefox": "3.0.9",
    "@browser-logos/safari": "2.1.0",
    "@fontsource/inter": "4.5.12",
    "@hsjs/svelte-icons": "0.0.1-rc.4",
    "@sveltejs/adapter-node": "1.0.0-next.96",
    "@sveltejs/kit": "1.0.0-next.507",
    "@sveltejs/package": "1.0.0-next.5",
    "@types/better-sqlite3": "7.6.0",
    "@types/debug": "4.1.7",
    "@types/estree": "1.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "autoprefixer": "10.4.12",
    "domhandler": "5.0.3",
    "esbuild": "0.15.10",
    "esbuild-register": "3.3.3",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "nanoid": "4.0.0",
    "postcss": "8.4.17",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.1",
    "sade": "1.8.1",
    "sass": "1.55.0",
    "svelte": "3.50.1",
    "svelte-check": "2.9.1",
    "svelte-file-dropzone": "1.0.0",
    "svelte-preprocess": "4.10.7",
    "tiny-invariant": "1.3.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "2.4.0",
    "tsx": "3.9.0",
    "typescript": "4.8.4",
    "undici": "5.10.0",
    "uvu": "0.5.6",
    "vite": "3.1.4"
  },
  "type": "module",
  "dependencies": {
    "@node-rs/argon2": "1.2.0",
    "@node-rs/crc32": "1.5.1",
    "better-sqlite3": "7.6.2",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "html-metadata-parser": "2.0.4",
    "htmlparser2": "8.0.1",
    "zod": "3.19.1"
  }
}
