{
  "name": "cherry",
  "version": "0.0.8",
  "private": true,
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-svelte"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "vite dev",
    "test": "NODE_OPTIONS='--import tsx' uvu src .test.ts",
    "do": "NODE_OPTIONS='--import tsx' node",
    "test:1": "uvu -r esbuild-register -r tsconfig-paths/register",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "sync": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "bundle:cli": "esbuild src/cli/index.ts --bundle --minify --platform=node --target=node16 --outfile=cherry",
    "cherry": "tsx src/cli/index.ts",
    "lint": "prettier --check src scripts && eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "prettier:fix": "prettier --write src scripts",
    "fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "devDependencies": {
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/firefox": "3.0.10",
    "@browser-logos/safari": "2.1.0",
    "@fontsource/inter": "5.0.18",
    "@hsjs/svelte-icons": "0.0.1-rc.4",
    "@sveltejs/adapter-node": "5.2.0",
    "@sveltejs/kit": "2.5.17",
    "@sveltejs/package": "2.3.2",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@types/better-sqlite3": "7.6.10",
    "@types/debug": "4.1.12",
    "@types/estree": "1.0.5",
    "@types/tar": "6.1.13",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "autoprefixer": "10.4.19",
    "domhandler": "5.0.3",
    "esbuild": "0.21.5",
    "esbuild-register": "3.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-svelte": "2.41.0",
    "nanoid": "5.0.7",
    "postcss": "8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-svelte": "3.2.5",
    "sade": "1.8.1",
    "sass": "1.77.6",
    "svelte": "4.2.18",
    "svelte-check": "3.8.4",
    "svelte-file-dropzone": "2.0.7",
    "tiny-invariant": "1.3.3",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.3",
    "tsx": "4.16.0",
    "typescript": "5.5.2",
    "undici": "6.19.2",
    "uvu": "0.5.6",
    "vite": "5.3.2"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "3.606.0",
    "@node-rs/argon2": "1.8.3",
    "@node-rs/crc32": "1.10.3",
    "better-sqlite3": "11.1.1",
    "csv-stringify": "6.5.0",
    "date-fns": "3.6.0",
    "debug": "4.3.5",
    "dotenv": "16.4.5",
    "html-metadata-parser": "2.0.4",
    "htmlparser2": "9.1.0",
    "tar": "7.4.0",
    "zod": "3.23.8"
  }
}
