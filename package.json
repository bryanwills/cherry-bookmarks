{
  "name": "cherry",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "scripts": {
    "start": "DEBUG=cherry:* NODE_ENV=development node -r 'global-agent/bootstrap' node_modules/@sveltejs/kit/svelte-kit.js dev",
    "dev": "svelte-kit dev",
    "postinstall": "node scripts/font.mjs",
    "build": "svelte-kit build",
    "postbuild": "tree .vercel_build_output",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src scripts && eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "prettier:fix": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src scripts",
    "fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "devDependencies": {
    "@fontsource/inter": "4.5.10",
    "@hsjs/svelte-icons": "0.0.1-rc.4",
    "@sveltejs/adapter-node": "1.0.0-next.76",
    "@sveltejs/kit": "1.0.0-next.345",
    "@types/better-sqlite3": "7.5.0",
    "@types/debug": "4.1.7",
    "@types/jsonwebtoken": "8.5.8",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "global-agent": "3.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "sass": "1.52.1",
    "svelte": "3.48.0",
    "svelte-check": "2.7.1",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.7.2"
  },
  "type": "module",
  "dependencies": {
    "better-sqlite3": "7.5.3",
    "debug": "4.3.4",
    "dotenv": "16.0.1",
    "html-metadata-parser": "2.0.4",
    "jsonwebtoken": "8.5.1",
    "node-fetch": "3.2.4",
    "tiny-invariant": "1.2.0"
  }
}
